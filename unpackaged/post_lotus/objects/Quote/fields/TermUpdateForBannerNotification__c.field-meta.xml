<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>TermUpdateForBannerNotification__c</fullName>
    <description>Returns 1 if the value is only Critical, 2 if the value is Standard, for all other scenarios it is returning 3.</description>
    <externalId>false</externalId>
    <formula>IF(
    INCLUDES(TermUpdatesRequired__c,&apos;Critical&apos;), &apos;1&apos;,
        IF(
            AND(
                INCLUDES(TermUpdatesRequired__c,&apos;Standard&apos;),
                NOT(INCLUDES(TermUpdatesRequired__c,&apos;Critical&apos;))
              ), 
              &apos;2&apos;,
              &apos;3&apos;
        )
    )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Term Update for Banner Notification</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
